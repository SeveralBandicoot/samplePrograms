//Passing a substitute Pointer for the array

void passPointer(int*, int);
int main() {
int box[7]{ 0,3,4,1,5,2,6 };
int* bPtr;
bPtr = &box[0];
passPointer(bPtr, pn + 5);
Return 0; }
void passPointer(int* bPtr , int pn)
{
for (int i = 0; i < pn; i++)
{
cout << " " << *bPtr << " -- ";
*bPtr+=5;
bPtr++;
}
}